<?xml version="1.0"?>
<!DOCTYPE layout-templates PUBLIC "-//Liferay//DTD Layout Templates 6.2.0//EN" "http://www.liferay.com/dtd/liferay-layout-templates_6_2_0.dtd">

<layout-templates>
	<custom>
		<layout-template id="WorkdashLayout" name="WorkdashLayout">
			<template-path>/WorkdashLayout.tpl</template-path>
			<wap-template-path>
				/WorkdashLayout.wap.tpl
			</wap-template-path>
			<thumbnail-path>/WorkdashLayout.png</thumbnail-path>
		</layout-template>
		<layout-template id="workdashtemplate"
			name="WorkdashTemplate">
			<template-path>/workdashtemplate.tpl</template-path>
			<wap-template-path>
				/workdashtemplate.wap.tpl
			</wap-template-path>
			<thumbnail-path>/workdashtemplate.png</thumbnail-path>
		</layout-template>
	</custom>
</layout-templates>